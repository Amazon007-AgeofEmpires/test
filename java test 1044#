package com.company;

import java.util.Scanner;

class Student{
   public String name;
   private int age;
   public int no;

   public Student(String name, int age,int no){
       super();
       this.name = name;
       this.age = age;
       this.no = no;
   }
   public String getName(){
       return name;
   }
   public void setName(String name){
       this.name = name;
   }
   public int getAge(){
       return age;
   }
   public void setAge(int age){
       this.age = age;
   }
    public int getNo(){
        return no;
    }
    public void setNo(int no){
        this.no = no;
    }


}
public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        Student student = new Student(scanner.next(),scanner.nextInt(),scanner.nextInt());
        System.out.println("Name:"+student.name + " Age:" + student.getAge() + " No:" + student.no);



    }
}
